<Window x:Class="WpfApplication1.Login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Login" Height="300" Width="300">
    <Grid>
        <Label Content="Documento&#xD;&#xA;" HorizontalAlignment="Left" Height="31" Margin="32,64,0,0" VerticalAlignment="Top" Width="92"/>
        <Label Content="Contraseña" HorizontalAlignment="Left" Margin="32,106,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="0.515,-2.063"/>
        <TextBox x:Name="txtDocumento" HorizontalAlignment="Left" Height="23" Margin="124,64,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label Content="Ingreso al sistema:" HorizontalAlignment="Left" Height="32" Margin="103,10,0,0" VerticalAlignment="Top" Width="141" FontSize="16"/>
        <Image HorizontalAlignment="Left" Height="34" Margin="43,17,0,0" VerticalAlignment="Top" Width="31" Source="/WpfApplication1;component/Imagenes/login.jpg" RenderTransformOrigin="0.949,-3.448"/>
        <Button x:Name="btIngresar" Content="Ingresar" HorizontalAlignment="Left" Margin="103,167,0,0" VerticalAlignment="Top" Width="96" RenderTransformOrigin="0.045,0.438" Click="btIngresar_Click"/>
        <PasswordBox x:Name="txtPassword" HorizontalAlignment="Left" Height="23" Margin="124,109,0,0" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
